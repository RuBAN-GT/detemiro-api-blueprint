{
  "name": "detemiro-api-action-manager",
  "version": "2.0.0",
  "author": "Dmitry Ruban",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "api:dev": "NODE_ENV=development nest start --watch --exec 'node -r tsconfig-paths/register -r ts-node/register ./src/api.ts'",
    "api:debug": "NODE_ENV=development nest start --debug --watch --exec 'node -r tsconfig-paths/register -r ts-node/register ./src/api.ts'",
    "api:prod": "NODE_ENV=production node dist/api",
    "worker:dev": "NODE_ENV=development nest start --watch --exec 'node -r tsconfig-paths/register -r ts-node/register ./src/worker.ts'",
    "worker:debug": "NODE_ENV=development nest start --debug --watch --exec 'node -r tsconfig-paths/register -r ts-node/register ./src/worker.ts'",
    "worker:prod": "NODE_ENV=production node dist/worker",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.14",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.14",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/microservices": "^11.1.14",
    "@nestjs/platform-express": "^11.1.14",
    "@nestjs/platform-fastify": "^11.1.14",
    "amqp-connection-manager": "^4.1.15",
    "amqplib": "^0.10.9",
    "axios": "^1.13.5",
    "bluebird": "^3.7.2",
    "cache-manager": "^6.4.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "cqrs-proxy": "0.0.9",
    "detemiro-api-db-adapter": "workspace:*",
    "detemiro-logger": "^2.5.0",
    "detemiro-logger-nestjs": "^2.5.0",
    "detemiro-logger-node": "^2.5.0",
    "fastify": "^5.7.4",
    "ioredis": "^5.9.3",
    "redis": "^5.11.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.14",
    "@types/bluebird": "^3.5.42",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.33",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
